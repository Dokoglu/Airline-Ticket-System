package com.eticket.ETicket.demo.model;


public class Ticket extends Flight {
	
	private int ticketID;
	private float price;
	
	public Ticket(int flightID, int durationOfFlight, String departureAirport, 
			String arrivalAirport,String departureCity, String arrivalCity, 
			int ticketID, float price) {
		super(flightID, durationOfFlight, departureAirport, arrivalAirport, departureCity, arrivalCity);
		this.ticketID = ticketID;
		this.price = price;
	}

	public Ticket() {
		
	}

	public int getTicketID() {
		return ticketID;
	}

	public void setTicketID(int ticketID) {
		this.ticketID = ticketID;
	}

	public float getPrice() {
		return price;
	}

	public void setPrice(float price) {
		this.price = price;
	}
	
	

}
